if theme.graphviz.enable == true
  script.
    load('/libs/viz/2.1.2/viz.js').then(() => {
      let graphs = document.querySelectorAll('.graphviz')

      if (graphs.length > 0) {
        for (let graph of graphs) {
          let svg = undefined;
          try {
            svg = Viz(graph.textContent.replaceAll('â€“', '--'), 'svg');
          } catch(e) {
            svg = `<pre class="error">${e}</pre>`;
          }
          graph.outerHTML = svg;
        }
      }
    });
    
